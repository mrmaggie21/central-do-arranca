      }
    },
    "saude": {
      "baseUrl": "https://api-saudediaria.entregadigital.app.br/api/v1/app/resetpassword",
      "timeout": 15000,
      "maxRetries": 3,
      "rateLimit": {
        "enabled": true,
        "maxRequestsPerMinute": 60,
        "retryAfterSeconds": 60
      }
    },
    "workbuscas": {
      "baseUrl": "https://completa.workbuscas.com/api",
      "timeout": 15000,
      "rateLimit": {
        "enabled": true,
        "maxRequestsPerMinute": 60,
        "retryAfterSeconds": 60
      }
    },
    "telesena": {
      "baseUrl": "https://api.telesena.com.br/api/v2/customer/password-recovery/options",
      "timeout": 15000,
      "maxRetries": 3,
      "rateLimit": {
        "enabled": true,
