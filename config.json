  "metrics": {
    "enabled": true,
    "collectionInterval": 5000,
    "saveInterval": 60000,
    "outputPath": "./logs/metrics.json"
  },
  "cache": {
    "enabled": true,
    "baseDir": ".cache",
    "proxies": {
      "gemeos": "proxies-gemeos.json",
      "saude": "proxies-saude.json",
      "telesena": "proxies-telesena.json"
