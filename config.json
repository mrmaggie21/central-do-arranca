{
  "app": {
    "name": "Central do Arranca",
    "version": "1.5.1",
    "author": "Mr.Robot"
  },
  "apis": {
    "gemeos": {
      "baseUrl": "https://dashboard.gemeosbrasil.me/api/ver-numeros",
      "timeout": 15000,
      "maxRetries": 3,
      "rateLimit": {
        "enabled": true,
        "maxRequestsPerMinute": 60,
