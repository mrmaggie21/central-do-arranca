  },
  "proxies": {
    "webshare": {
      "apiUrl": "https://proxy.webshare.io/api/v2/proxy/list/",
      "maxProxies": 1000,
      "pageSize": 25,
      "cacheExpiryHours": 24,
      "testTimeout": 3000,
      "testBatchSize": 10,
      "rotate": {
        "enabled": true,
        "username": "qkwskesg-rotate",
        "password": "8f5e27vxgc2y",
        "host": "p.webshare.io",
        "port": 80,
        "protocol": "socks5"
      }
    },
    "useForeignOnly": false,
    "filterByCountry": false,
    "skipTestOnLoad": true
  },
  "checkers": {
    "gemeos": {
      "batchSize": 20,
      "delay": 2000,
      "timeout": 10000
    },
    "saude": {
      "batchSize": 20,
      "delay": 2000,
      "timeout": 10000
    },
    "workbuscas": {
      "delay": 2000,
      "timeout": 15000
    },
    "telesena": {
      "batchSize": 20,
      "delay": 2000,
      "timeout": 10000
    }
  },
  "security": {
    "ssl": {
      "rejectUnauthorized": false,
      "secureProtocol": "TLSv1_2_method",
      "minTlsVersion": "1.2"
