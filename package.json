    "adm-zip": "^0.5.16",
    "axios": "^1.13.1",
    "chalk": "^4.1.2",
    "fs-extra": "^11.3.2"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "electron-packager": "^17.1.1"
  },
  "engines": {
